{
  "name": "PR-Shoot-APIs",
  "version": "1.1.0",
  "description": "",
  "main": "dist/src/app.js",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "eslint --fix . --ext .ts && prettier --write \"**/*.+(js|ts|json)\"",
    "clean": "rm -rf ./dist",
    "copy-files": "mkdir ./dist/public && ncp ./public ./dist/public",
    "build": "npm run clean && tsc",
    "prestart": "npm run build",
    "start": "node ./dist/src/app.js",
    "test": "mocha test/*",
    "dev": "nodemon --watch 'src/**/*.ts' --exec  ts-node src/app.ts",
    "local": "nodemon --watch src/**/*.ts --exec  NODE_ENV=local ts-node src/app.ts",
    "dev:sync": "nodemon --watch 'src/**/*.ts' --exec DB_SYNC='true' ts-node src/app.ts 'true' ",
    "dev:clear": "DB_CLEAR='true' ts-node src/app.ts",
    "local:sync": "nodemon --watch 'src/**/*.ts' --exec DB_SYNC='true' NODE_ENV=local ts-node src/app.ts 'true' ",
    "local:clear": "NODE_ENV=local DB_CLEAR='true' ts-node src/app.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts",
    "pretty": "prettier --write \"**/*.+(js|ts|json)\"",
    "ts-node": "ts-node",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:create": "npm run typeorm migration:create -- -n userMigration",
    "typeorm:run": "npm run typeorm migration:run -- -n userMigration",
    "ncp": "^2.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.24.0",
    "body-parser": "^1.19.2",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.3",
    "express-validator": "^6.14.0",
    "nodemailer": "^6.7.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^16.18.3",
    "@types/nodemailer": "^6.4.4",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.12.0",
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.1",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  }
}
